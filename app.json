{
  "name": "nullstar",
  "cwd": "/home/node/nullstar",
  "user": "node",
  "script": "index.js",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "/srv/nullstar/logs/stderr.log",
  "out_file": "/srv/nullstar/logs/stdout.log",
  "pid_file": "/srv/nullstar/nullstar.pid",
  "max_restarts": 10,
  "watch": false,
  "instances": "max",
  "no_daemon": true,
  "merge_logs": true,
  "exec_interpreter": "node",
  "exec_mode": "cluster",
  "env": {
    "NODE_ENV": "production"
  }
}